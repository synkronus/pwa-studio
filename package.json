    "@angular/core": "~13.3.6",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.3.6",
    "@angular/localize": "^13.3.6",
    "@angular/platform-browser": "~13.3.6",
    "@angular/platform-browser-dynamic": "~13.3.6",
    "@angular/router": "~13.3.6",
    "@angular/service-worker": "^13.3.6",
    "@codewithdan/observable-store": "^2.2.14",
    "@ngx-formly/core": "^6.0.0-next.9",
    "@ngx-formly/primeng": "^6.0.0-next.9",
