{
  "name": "spa",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "config": "ts-node ./config/set-env.ts",
    "build": "npm run config && ng build",
    "deployx": "npm run write:env -s && ng deploy",
    "deployGHP": "ng build --configuration production --output-path docs --base-href=/pwastudio/ ",
    "build-pdn": "ng build --configuration production && javascript-obfuscator dist --output dist",
    "watch": "ng build --watch --configuration development",
    "post-build": "node ./build/post-build.js",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "test-pre-build": "ng test --watch=false --code-coverage",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.6",
    "@angular/cdk": "^13.3.6",
